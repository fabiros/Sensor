{
    "extends": ["airbnb-base", "prettier"],
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "indent": [2, 4],
      "consistent-return": 0, // Ignore
      "no-unused-vars": ["warn", { "vars": "all", "args": "all", "ignoreRestSiblings": false }]
    },
    "plugins": ["prettier"] 
}